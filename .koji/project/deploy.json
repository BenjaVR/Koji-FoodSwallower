{
    "deploy": {
        "subdomain": "foodswallower.withkoji.com",
        "frontend":  {
            "output":  "frontend/dist",
            "commands":  [
                "cd frontend",
                "npm install",
                "export NODE_ENV=production && npm run build"
            ]
        },
        "backend":  {
            "output":  "backend/dist",
            "commands":  [
                "cd backend",
                "npm install",
                "export NODE_ENV=production && npm run build"
            ]
        }
    }
}
