{
  "name": "koji-food-swallower-frontend",
  "private": true,
  "scripts": {
    "clean": "rimraf ./dist/**",
    "watch:dev": "parcel ./src/index.html",
    "watch:vcc": "koji-vcc watch-ts",
    "start": "npm-run-all clean --parallel watch:*",
    "build": "npm-run-all clean build:parcel",
    "build:parcel": "cross-env NODE_ENV=production parcel build ./src/index.html",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@withkoji/vcc": "1.1.3",
    "classnames": "2.2.6",
    "howler": "2.1.3",
    "phaser": "3.22.0",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "@types/howler": "2.1.2",
    "@types/jest": "24.9.0",
    "@types/node": "13.1.7",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "cross-env": "6.0.3",
    "jest": "24.9.0",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.4",
    "postcss-modules": "1.5.0",
    "rimraf": "3.0.0",
    "sass": "1.25.0",
    "ts-jest": "24.3.0",
    "typescript": "3.7.5",
    "typescript-plugin-css-modules": "2.1.2"
  }
}
